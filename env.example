# -------- Database (Postgres) --------
POSTGRES_USER=your_username
POSTGRES_PASSWORD=your_password
POSTGRES_DB=database_name
POSTGRES_PORT=5432 # local postgres port 

# Airflow metadata DB (same Postgres instance, different database)
AIRFLOW_DB=airflow_db_name
AIRFLOW_USER=airflow_username
AIRFLOW_PASSWORD=airflow_user_password

# SQLAlchemy URL used by ETL and frontend to connect to Postgres. 5432 is docker postgres port
DB_URL=postgresql+psycopg2://${POSTGRES_USER}:${POSTGRES_PASSWORD:}@db:5432/${POSTGRES_DB}

#  ETL Parameters 
TICKERS=AAPL,MSFT,NVDA
START_DATE=2019-01-01
END_DATE=
INTERVAL=1d
TABLE_NAME=price_metrics

#  Airflow
AIRFLOW__CORE__LOAD_EXAMPLES=False
AIRFLOW__WEBSERVER__RBAC=True
AIRFLOW_ADMIN_USERNAME=admin_username
AIRFLOW_ADMIN_PASSWORD=admin_password
AIRFLOW_ADMIN_EMAIL=admin@example.com

# Airflow Security Keys
AIRFLOW_FERNET_KEY={generate_own_fernet_key}
AIRFLOW_SECRET_KEY={generate_own_secret_key}

# -------- Frontend --------
STREAMLIT_PORT=8501

